    <ItemGroup>
      <Folder Include="bin\Debug\net9.0-windows\" />
    </ItemGroup>

    <ItemGroup>
      <None Include="bin\Debug\net9.0-windows\Data\Bank.JSON" />
    </ItemGroup>

